<h1>管理画面のユーザー　一覧画面だよ！</h1>
<% @users.each do |user| %>
  <%= user.id %> : <%= user.name %>
  <%=link_to user.name, admin_users_path(user.id) %>
  <%= link_to 'ユーザー編集画面', edit_admin_user_path(user.id) %>
  <%= link_to 'ユーザーを削除する', admin_user_path(user.id),
                                  method: :delete, data: { confirm: 'Are you sure to delete this user?'} %>
<% end %>